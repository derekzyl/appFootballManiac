{
    "plugins": [
      ["module:react-native-dotenv"],
      ["module-resolver", {
        "root": ["./src"],
        "extensions":[
          ".ios.ts",
          ".android.ts",
          ".ts",
          ".ios.tsx",
          ".android.tsx",
          ".tsx",
          ".jsx",
          ".js",
          ".json"
        ],
        "alias": {
          "@lib": "./src/lib",
          "@components": "./src/components",
          "@src": "./src",
          "@hooks": "./src/hooks",
          "@theme":"./src/theme",
          "@screens": "./src/screens",
          "@assets": "./src/assets"
        }
      }]
    ]
  }